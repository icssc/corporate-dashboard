{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "isolatedModules": true,
    "module": "ES2022",
    "moduleResolution": "Node",
    "noImplicitReturns": true,
    "outDir": "dist",
    "resolveJsonModule": true
  }
}
